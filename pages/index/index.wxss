/**index.wxss**/
@import "../../template/goodslist/goodslist.wxss";
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container {
  padding: 0;
}

.cleans {
  clear: both;
}

.big {
  display: flex;
  flex-wrap: wrap;
  background: #f8f8f8;
  padding-bottom: 0;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.testbox{
  display: flex;
  flex-wrap:wrap;
  margin-top:160rpx;
}

.za_a {
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 20rpx;
  font-size: 16px;
}
.za_b {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  border-top: 1rpx solid #f6f5f5;
  padding:15rpx 15rpx 0 15rpx;
  font-size: 12px;
}
.za_b_a {
  width: 25%;
  background: #f6f5f5;
  text-align: center;
  border-radius: 10rpx;
  padding: 10rpx;
  margin: 0 15rpx;
}
.za_ab {
  display: flex;
  flex-wrap:wrap;
  margin-bottom: 10rpx;
  padding: 0 35rpx;
}
.za {
  width: 25%;
  text-align: center;
  flex-wrap: wrap;
}

.loading {
  width: 100%;
  padding: 10rpx;
  text-align: center; 
}

.loading:before {
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  animation: rotate 1s linear infinite;
}

.loading.complete:before {
  display: none;
}

.tops {
  position: fixed;
  right: 50rpx;
  top: 950rpx;
  width: 44rpx;
  z-index: 20;
}

.crowd {
  float: left;
  font-size: 14px;
  color: #b5b5b5;
}

.money {
  float: right;
  font-size: 12px;
}

.top_img {
  width: 30rpx;
  display: inline-block;
}

.top_img2 {
  width: 32rpx;
  display: inline-block;
  margin-bottom: 8rpx;
}

.top_img3 {
  width: 22rpx;
  display: inline-block;
  vertical-align: middle;
  margin-bottom: 6rpx;
}

.actives {
  background: #666 !important;
}
 
/*人群弹出窗  */

.sex {
  position: fixed;
  top: 0;
  bottom: 0;
  background: #f8f8f8;
  width: 100%;
  z-index: 99;
}

.header {
  height: 108rpx;
  background: #fff;
}

.left_img {
  width: 34rpx;
  margin-top: 40rpx;
  margin-left: 40rpx;
  float: left;
}

.center {
  width: 500rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 108rpx;
}

.corwd {
  margin-top: 50rpx;
}

.xuans {
  background: #fff;
  line-height: 100rpx;
  border-bottom: 1px solid #f5f5f5;
}

.check_img {
  width: 45rpx;
  display: inline-block;
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.check_img .img {
  vertical-align: middle;
}

.bottom_p {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.zx {
  text-align: center;
  width: 50%;
