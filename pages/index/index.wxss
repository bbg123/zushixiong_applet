  line-height: 140rpx;
}

.bottom_l {
  background: #eee;
}

.bottom_r {
  color: #fff;
  background: #f33e35;
}

/*选品牌弹窗  */

.tang {
  position: fixed;
  top: 0;
  left: 0;
  overflow-y: scroll;
  background: #fff;
  width: 100%;
  height: 100%;
  z-index: 55;
}

.tang {
  background: #f8f8f8;
}

.logo {
  width: 90rpx;
  margin-left: 30rpx;
}

.top_img .img{
  vertical-align: middle;
}

.top {
  padding-left: 30rpx;
}

.brand_name {
  margin-left: 30rpx;
  line-height: 100rpx;
}

.bottom {
  position: fixed;
  left: 0 ;
  bottom: 0;
  width: 100%;
}

.checkbox {
  height: 100%;
  margin-top: 102rpx;
}

.view {
  width: 50%;
  float: left;
  text-align: center;
  line-height: 100rpx;
}

.view:nth-child(1) {
