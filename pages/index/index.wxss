}

.tos_img {
  width: 100%;
  background: #fff;
  padding: 30rpx 40rpx;
  position: fixed;
  top: 0;
  left: 0;
  box-sizing: border-box;
  border-top: 1px solid #f8f8f8;
  border-bottom: 1px solid #f8f8f8;
  z-index: 20;
}

.tos_img view {
  width: 30rpx;
  float: right;
}

.body {
  background: #fff;
}

checkbox {
