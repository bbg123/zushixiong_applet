.getroll {
  width: 95%;
}
.getroll_list {
  width: 100%;
  background: #fff;
  display: flex;
  margin:20rpx;
}
.getroll_list_tip {
  width: 33%;
  position: relative;
  border-right: 1px dashed #ccc;
  display: flex;
  flex-direction:column;
  justify-content:center;
  align-items: center;
  padding: 30rpx 10rpx 40rpx;
  color: #fe825c;
}
.getroll_list_tip_money {
  font-size: 36px;
  line-height:1;
}
.getroll_list_tip_money_fh {
  font-size: 16px;
}
.getroll_list_tip_type {
  font-size: 14px;
}
.getroll_list_tip_icon {
  width: 40%;
  position: absolute;
  left: 0;
  top: 0;
}
.getroll_list_txt {
  width: 65%;
  display: flex;
  flex-direction:column;
  justify-content:center;
  padding: 0 20rpx;
  font-size: 12px;
  position: relative;
}
.getroll_list_txt_name {
  font-size: 16px;
  margin-bottom:10rpx;
}
.getroll_list_txt_name1 {
  font-size: 14px;
}
.getroll_list_txt_time {
  color: #888;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.getroll_list_txt_pos{
  background: #f8f8f8;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  position: absolute;
  left: -15rpx;
}

.getroll_list_isGet {
  position: absolute;
  background-color: #f33e36;
  padding:8rpx 15rpx;
  right: 10rpx;
  top: 50%;
  transform: translateY(-50%);
  color: #fff;
  border-radius: 15px;
}

.getroll_list_isGet1 {
  background-color: #888;
}