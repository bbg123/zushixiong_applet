.footprint_main {
  margin-top: 15rpx;
  background: #fff;
}
.footprint_main_title{
  padding: 30rpx 20rpx;
  font-size: 30rpx;
  color: #666;
}
.footprint_main_content{
  height: 270rpx;
  display: flex;
  border-top: 1rpx solid #f4f4f4;
  padding: 10rpx 30rpx;
  align-items:center;
}

.footprint_main_content_img {
  width: 33%;
  border: 1rpx solid #f4f4f4;
}

.footprint_main_content_introduce {
  width: 66%;
  padding:0 10rpx;
  font-size: 32rpx;
}

.footprint_main_content_introduce_name {
  margin-top: 10rpx;
}

.footprint_main_content_introduce_advertising {
  font-size: 24rpx;
  color: #ccc;
  margin: 10rpx 0;
}

.footprint_main_content_introduce_price {
  color: #666;
  font-size: 28rpx;
}